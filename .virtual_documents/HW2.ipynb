def cumsum(t):
    result = []
    sum = 0
    for i in t:
        sum += i
        result.append(sum)
    return result


cumsum([1, 2, 3, 4])


cumsum(range(12))





def is_anagram(word1, word2):
    word1 = word1.replace(" ", "").lower()
    word2 = word2.replace(" ", "").lower()
    return sorted(word1) == sorted(word2)


is_anagram("hello", "o hell")


is_anagram("dormitory" , "dirty room")


is_anagram("dormitory" , "dirty rooms")


