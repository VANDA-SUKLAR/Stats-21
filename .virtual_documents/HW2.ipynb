def has_duplicates(t):
    sorted_list = sorted(t) 
    for i in range(len(sorted_list) - 1):
        if sorted_list[i] == sorted_list[i + 1]:
            return True  
    return False


has_duplicates(['a','b','c'])


has_duplicates(['a','b','b','c'])


has_duplicates(['a','b','c','a'])





